    $("input[type='radio']").click(function () {
        // Dạng check radio Có / Không
        if (this.id.indexOf("rd_value_") != -1) {
            var radio_ID = $(this).attr('id');
            var name = $(this).attr('name');
            var base_ID = name.substring(9, name.length);
